doi = {10.1016/j.csl.2014.03.003},
file = {:Users/lauren/Documents/Papers/Drugman et al., 2014, Glottal source processing From analysis to applications.pdf:pdf},
issn = {10958363},
journal = {Computer Speech and Language},
keywords = {Excitation signal,Glottal flow,Glottal source,Residual signal,Speech analysis,Speech processing},
number = {5},
pages = {1117--1138},
title = {{Glottal source processing: From analysis to applications}},
